    </actionOverrides>
    <allowInChatterGroups>false</allowInChatterGroups>
    <compactLayoutAssignment>SYSTEM</compactLayoutAssignment>
    <deploymentStatus>Deployed</deploymentStatus>
    <description>Represents an Activity Type related to case. This object is managed by business admin to store the Activity Type values.</description>
    <enableActivities>true</enableActivities>
    <enableBulkApi>true</enableBulkApi>
    <enableEnhancedLookup>true</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>true</enableReports>
    <enableSearch>true</enableSearch>
    <enableSharing>true</enableSharing>
    <enableStreamingApi>true</enableStreamingApi>
    <externalSharingModel>ReadWrite</externalSharingModel>
    <fields>
        <fullName>Active__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Determines whether this record is active</description>
        <externalId>false</externalId>
        <label>Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ActivityCode__c</fullName>
        <description>The unique external Id for identifying Activity Codes</description>
        <externalId>false</externalId>
        <label>Activity Code</label>
        <length>80</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Code_Portuguese__c</fullName>
        <externalId>false</externalId>
        <label>Activity Code Portuguese</label>
        <length>80</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Name_Formula__c</fullName>
        <description>Based on the current language, the activity name is either the portuguese name or the english name. If the portuguese name is blank, then show english name</description>
        <externalId>false</externalId>
        <formula>CASE($Label.YAHS_CurrentLanguage,&apos;pt_PT&apos;, IF(ISBLANK(Activity_Name_Portuguese__c), Name,Activity_Name_Portuguese__c) ,&apos;en_US&apos;, Name, Name)</formula>
        <label>Activity Type Name</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Name_Portuguese__c</fullName>
        <externalId>false</externalId>
        <label>Activity Name Portuguese</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>AvailableToUser__c</fullName>
        <defaultValue>false</defaultValue>
        <description>The checkbox field which holds the information if Activity Type should be available for User to select on the Case. This flag is defined for to make users not be able to select the inbound Activity type</description>
        <externalId>false</externalId>
        <label>Available To User</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Description__c</fullName>
        <description>The description of the Activity Type selected for the record</description>
        <externalId>false</externalId>
        <label>Description</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>HSvc_Active__c</fullName>
        <externalId>false</externalId>
        <formula>IF( HSvc_ExpiryDate__c  &lt;  TODAY() , False, True)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>HSvc_EffectiveDate__c</fullName>
        <defaultValue>today()</defaultValue>
        <externalId>false</externalId>
        <label>Effective Date</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>HSvc_ExpiryDate__c</fullName>
        <externalId>false</externalId>
        <label>Expiry Date</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>RecordType__c</fullName>
        <externalId>false</externalId>
        <label>Record Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Business Support Case</fullName>
                    <default>false</default>
                    <label>Business Support Case</label>
                </value>
                <value>
                    <fullName>Consumer Customer Case</fullName>
                    <default>false</default>
                    <label>Consumer Customer Case</label>
                </value>
                <value>
                    <fullName>Install Work Order</fullName>
                    <default>false</default>
                    <label>Install Work Order</label>
                </value>
                <value>
                    <fullName>Enterprise Customer Case</fullName>
                    <default>false</default>
                    <label>Enterprise Customer Case</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>isActive__c</fullName>
        <description>is active will be true if activity type is not expired yet</description>
        <externalId>false</externalId>
        <formula>IF(HSvc_EffectiveDate__c &lt;= TODAY() &amp;&amp; TODAY() &lt;=  HSvc_ExpiryDate__c, true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Is Active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Activity Type</label>
    <listViews>
        <fullName>All</fullName>
        <columns>CREATED_DATE</columns>
        <columns>RecordType__c</columns>
        <columns>ActivityCode__c</columns>
        <columns>NAME</columns>
        <columns>Description__c</columns>
        <columns>AvailableToUser__c</columns>
        <columns>HSvc_EffectiveDate__c</columns>
        <columns>HSvc_ExpiryDate__c</columns>
        <columns>HSvc_Active__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
        <language>en_US</language>
    </listViews>
    <nameField>
        <label>Activity Type Name</label>
        <type>Text</type>
    </nameField>
    <pluralLabel>Activity Types</pluralLabel>
    <searchLayouts>
        <customTabListAdditionalFields>RecordType__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>ActivityCode__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>Description__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>AvailableToUser__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>HSvc_EffectiveDate__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>HSvc_ExpiryDate__c</customTabListAdditionalFields>
        <customTabListAdditionalFields>HSvc_Active__c</customTabListAdditionalFields>
        <excludedStandardButtons>ChangeOwner</excludedStandardButtons>
        <lookupDialogsAdditionalFields>ActivityCode__c</lookupDialogsAdditionalFields>
        <lookupDialogsAdditionalFields>RecordType__c</lookupDialogsAdditionalFields>
    </searchLayouts>
    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>HSvc_CannotChangeActivityCode</fullName>
        <active>true</active>
        <description>Once Activity Code is defined, it should not be allowed to change</description>
        <errorConditionFormula>ISCHANGED( ActivityCode__c )</errorConditionFormula>
        <errorDisplayField>ActivityCode__c</errorDisplayField>
        <errorMessage>Activity Code cannot be changed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>HSvc_ExpiryDateCannotBeSetUpInPast</fullName>
        <active>true</active>
        <errorConditionFormula>OR(HSvc_ExpiryDate__c &lt; TODAY(),HSvc_ExpiryDate__c &lt; HSvc_EffectiveDate__c)</errorConditionFormula>
        <errorDisplayField>HSvc_ExpiryDate__c</errorDisplayField>
        <errorMessage>Expiry Date cannot be set in past and cannot be less than the effective date</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
